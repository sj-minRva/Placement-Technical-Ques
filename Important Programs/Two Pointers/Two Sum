def TwoSum(arr, target):
    l = 0
    r = len(arr) - 1
    
    while l < r :
        Sum = arr[l] + arr[r]
        if Sum == target:
            return [l+1, r+1]        
        else:
            if target < Sum:
                r -= 1
            else:
                l += 1
    return "Not Found"
                

    
arr = [1, 3, 4, 8, 9]
target = 5

result = TwoSum(arr, target)
print(result)
    
